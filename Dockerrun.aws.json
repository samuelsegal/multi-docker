{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "samuelsegal/multi-client",
			"hostname": "client",
			"essential": false,
			"portMappings": [
				{
					"hostPort": 3000,
					"containerPort": 3000
				}
			],
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "samuelsegal/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"memory": 128
		}
	]
}
